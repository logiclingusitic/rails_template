<div id="<%= dom_id order_item %>">
  <p>
    <strong>Product name:</strong>
    <%= order_item.product_name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= order_item.price %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= order_item.quantity %>
  </p>

  <p>
    <strong>Order:</strong>
    <%= order_item.order_id %>
  </p>

</div>
